### github

### -- PERSONAL/MAIN ACCOUNT --

Host github.com
	HostName github.com
	User git
	IdentityFile ~/.ssh/id_rsa

### -- WORK ACCOUNT --

Host github.com-ruby-dev
	HostName github.com
	User git
	IdentityFile ~/.ssh/ruby_dev_rsa

Host git.ruby-dev.jp
	HostName git.ruby-dev.jp
	User git
	IdentityFile ~/.ssh/ruby_dev_rsa

### general

Host *
  AddKeysToAgent yes
  IdentitiesOnly yes
  PreferredAuthentications publickey
  Compression yes
